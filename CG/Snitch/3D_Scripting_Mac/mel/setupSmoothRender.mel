global proc setupSmoothRender (){	// build selection list	string $selList[] = `ls "selection`;		// parse the array holding the selection list	for ( $eachItem in $selList )		{						// declare array to hold polySmoothFace nodes			string $smoothNodes[];			for ( $eachItem in $selList )				{					// build array holding all the items contributing					// to each object as we parse the array					string $objHistory[] = `listHistory $eachItem`;										// parse the history, check to see if					// it is a smoothing node					for ( $histNode in $objHistory )						if ( `nodeType $histNode` == "polySmoothFace" )							$smoothNodes[`size $smoothNodes`] = $histNode;				}						// parse the list of smoothing nodes			for ( $eachNode in $smoothNodes )				// check to see if the node already has the				// added attributes, prevents error				if (`attributeExists "renderDivisions" $eachNode` != 1)					{						addAttr							-longName renderDivisions							-attributeType long							-minValue 0							-maxValue 4							-defaultValue 0							$eachNode							;												// make the attribute keyable so that it appears in						// the Channel Box						setAttr -edit -keyable true ( $eachNode + ".renderDivisions" ) ;					}				}}