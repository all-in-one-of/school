//----------------------------------------------------------------------------------------------
//
// builds a neck based on the given joint chain
//
//----------------------------------------------------------------------------------------------


global proc string[] icQRreverseArray ( string $list[] )
{
	string $return[];

	for ($i = size($list) - 1; $i >= 0; $i --)
	{
		$return[size($return)] = $list[$i];
	}

	return $return;
}
